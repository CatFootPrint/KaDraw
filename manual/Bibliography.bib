@inproceedings{Riedy2011,
	__Markedentry = {[ejr:6]},
	Abstract = {Tackling the current volume of graph-structured data requires parallel
	tools. We extend our work on analyzing such massive graph data with
	the first massively parallel algorithm for community detection that
	scales to current data sizes, scaling to graphs of over 122 million
	vertices and nearly 2 billion edges in under 7300 seconds on a massively
	multithreaded Cray XMT. Our algorithm achieves moderate parallel
	scalability without sacrificing sequential operational complexity.
	Community detection partitions a graph into subgraphs more densely
	connected within the subgraph than to the rest of the graph. We take
	an agglomerative approach similar to Clauset, Newman, and Moore's
	sequential algorithm, merging pairs of connected intermediate subgraphs
	to optimize different graph properties. Working in parallel opens
	new approaches to high performance. On smaller data sets, we find
	the output's modularity compares well with the standard sequential
	algorithms.},
	Address = {Torun, Poland},
	Author = {E. Jason Riedy and Henning Meyerhenke and David Ediger and David A. Bader},
	Booktitle = {Proceedings of the 9th International Conference on Parallel Processing and Applied Mathematics},
	Date-Added = {2013-03-07 08:44:12 +0000},
	Date-Modified = {2013-03-07 08:44:14 +0000},
	Month = sep,
	Owner = {ejr},
	Timestamp = {2012.02.07},
	Title = {Parallel Community Detection for Massive Graphs},
	Year = {2011}}

